        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// If attacking player


if (_seesPlayer) {
    var angleToPlayer = point_direction(x, y, objShip.x, objShip.y);
    var distanceToPlayer = point_distance(x, y, objShip.x, objShip.y);

    _input_strafe = 0;
    if (distanceToPlayer &lt; 64) {
        // Thrust back
        _input_thrust = -1;
    } else if (distanceToPlayer &gt; 128) {
        // Thrust forwards
        _input_thrust = 1;
    } else {
        _input_thrust = 0;
        // Strafe
        _input_strafe = _strafe_target;
    }

    var hitProgress = 1 - min(1, _hitTimer / _hitTimerTime);
    image_angle = angleToPlayer + _hitAngleOffset * hitProgress;
}
</string>
          </argument>

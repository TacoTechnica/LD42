        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Do we see the player? and DESTROY if too far away.

var distSqr = sqr(x - objShip.x) + sqr(y - objShip.y);
if (!_seesPlayer) {
    if (distSqr &lt; sqr(500))
        _seesPlayer = true;
} else {
    if (distSqr &gt; sqr(1024))
        _seesPlayer = false;
}

if (distSqr &gt; sqr(3000)) {
    instance_destroy();
}


if (get_target_planet_atmosphere_density(x, y) &gt; 0.05) {
    _escaping = true;
    alarm[2] = 60;
    image_angle = point_direction(get_target_planet_x(), get_target_planet_y(), x, y);
}

if (_escaping) {
    _seesPlayer = false;
}
</string>

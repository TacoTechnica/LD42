            <string>/// Spawn enemies randomly

var chance;
if is_inside_goo_radius(objShip.x, objShip.y) {
    if instance_number(objEnemy) &lt; 7 {
        chance = 1 / 70;
    } else {
        chance = 0;
    }
} else {
    chance = 1 / (60 * 7);
}

if (_counter &lt; 60 * 10) {
    chance *= _counter / (60 * 10);
}

var dg = distance_to_goo_origin(objShip.x, objShip.y);
if (dg &lt; 6000) {
    chance *= 0.8 * (dg / 6000);
    chance -= 0.2;
}

if (game_is_tutorial()) chance = 0;

while (random(1) &lt; chance) {
    // Make an enemy appear
    var angleRandom = random(360);
    var xa = view_xview + view_wview/2 + lengthdir_x(_throwRadius, angleRandom);
    var ya = view_yview + view_hview/2 + lengthdir_y(_throwRadius, angleRandom);
    var moveAngle = angleRandom - 180 + random(40) - 20; // What angle we're traveling at?

    var moveSpeed = random(1) + 2;

    var enemy = instance_create(xa, ya, objEnemy);
    if instance_exists(enemy) {
        enemy._velX = lengthdir_x(moveSpeed, moveAngle);
        enemy._velY = lengthdir_y(moveSpeed, moveAngle);
    }
}

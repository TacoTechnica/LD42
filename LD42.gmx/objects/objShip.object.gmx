        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// If within planet: Draw arrow to ring course and ring course info

if (get_target_planet() != noone &amp;&amp; get_target_planet_rings_left() &gt; 0) {
    var rx = get_target_planet_ring_course_x();
    var ry = get_target_planet_ring_course_y();
    var angle = point_direction(x, y, rx, ry);
    var dx = lengthdir_x(16, angle);
    var dy = lengthdir_y(16, angle);

    draw_set_color(c_yellow);
    draw_arrow(x + dx, y + dy, x + dx*2, y + dy*2, 10);

    // Ring counter
    draw_set_font(font_small);
    draw_text_transformed(x + 16, y + 16, "Rings Left: " + string(get_target_planet_rings_left()), 1, 1, -90 + image_angle);
}
</string>
